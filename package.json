{
  "name": "generator-api",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "author": "Altamir Consulting, LLC",
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "prestart": "if [ ! -d node_modules ]; then npm install; fi && npm run build",
    "start": "node dist/bin/www",
    "serve": "npm run start",
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "dev": "nodemon",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit --config ./test/jest-e2e.json",
    "test:full": "npm run test:unit",
    "test:unit": "NODE_ENV=test jest --coverage --colors --detectOpenHandles --forceExit",
    "test:e2e": "NODE_ENV=test jest --coverage --colors --detectOpenHandles --forceExit --config ./test/jest-e2e.json",
    "test:lint": "NODE_ENV=test tslint 'src/**/*.ts'",
    "test:audit": "NODE_ENV=test yarn audit",
    "ts-node": "ts-node",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "cli": "ts-node -r tsconfig-paths/register src/config/cli/index.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "json"
    ]
  },
  "dependencies": {
    "@sentry/node": "^4.5.3",
    "apollo-server-express": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "class-validator": "^0.9.1",
    "commander": "^2.19.0",
    "compression": "^1.7.3",
    "cron": "^1.6.0",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.3",
    "handlebars": "^4.0.13",
    "helmet": "^3.15.0",
    "inquirer": "^6.2.2",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "tslib": "^1.9.3",
    "typeorm": "^0.2.11",
    "typescript": "^3.3.1",
    "validator": "^10.11.0",
    "winston": "^3.1.0",
    "winston-loggly-bulk": "^3.0.0",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@altamir/standards-tsconfig": "^1.0.3",
    "@altamir/standards-tslint": "^1.0.4",
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.5",
    "@types/jest": "^22.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mkdirp": "^0.5.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.18",
    "@types/passport": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "@types/walk": "^2.3.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-uuid": "^1.0.6",
    "istanbul-api": "^2.1.1",
    "istanbul-lib-coverage": "^2.0.3",
    "jest": "^24.1.0",
    "js-yaml": "^3.12.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "oclif": "^1.12.8",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "walk": "^2.3.14"
  }
}
